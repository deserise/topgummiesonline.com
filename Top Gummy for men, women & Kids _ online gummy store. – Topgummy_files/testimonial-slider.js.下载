$(document).ready(function(){"use strict";var f=document.getElementById("testim"),c=Array.prototype.slice.call(document.getElementById("testim-dots").children),n=Array.prototype.slice.call(document.getElementById("testim-content").children),a=document.getElementById("left-arrow"),s=document.getElementById("right-arrow"),g=4500,t=0,d=0,h,l,u,r,v=30;window.onload=function(){function i(e){for(var o=0;o<c.length;o++)n[o].classList.remove("active"),n[o].classList.remove("inactive"),c[o].classList.remove("active");e<0&&(e=t=n.length-1),e>n.length-1&&(e=t=0),d!=t&&n[d].classList.add("inactive"),n[e].classList.add("active"),c[e].classList.add("active"),d=t,clearTimeout(h),h=setTimeout(function(){i(t+=1)},g)}a.addEventListener("click",function(){i(t-=1)}),s.addEventListener("click",function(){i(t+=1)});for(var m=0;m<c.length;m++)c[m].addEventListener("click",function(){i(t=c.indexOf(this))});i(t),document.addEventListener("keyup",function(e){switch(e.keyCode){case 37:a.click();break;case 39:s.click();break;case 39:s.click();break;default:break}}),f.addEventListener("touchstart",function(e){l=e.changedTouches[0].clientX}),f.addEventListener("touchend",function(e){if(u=e.changedTouches[0].clientX,r=l-u,console.log(r),console.log(l),console.log(u),r>0+v)a.click();else if(r<0-v)s.click();else return})}});
